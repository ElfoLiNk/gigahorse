          ],
          "default": "EmptyBody()",
          "since": "0.1.0"
        },
        {
          "name": "headers",
          "type": "Map[String, List[String]]",
          "doc": [
            "The headers for this request"
          ],
          "default": "Map()",
          "since": "0.1.0"
        },
        {
          "name": "queryString",
          "type": "Map[String, List[String]]",
          "doc": [
            "The query string for this request"
          ],
          "default": "Map()",
          "since": "0.1.0"
        },
        {
          "name": "signatureOpt",
          "type": "SignatureCalculator?",
          "default": "None",
          "since": "0.1.0"
        },
        {
          "name": "authOpt",
          "type": "Realm?",
          "default": "None",
          "since": "0.1.0"
        },
        {
          "name": "followRedirectsOpt",
          "type": "boolean?",
          "doc": [
            "Whether this request should follow redirects. (Default: `None`)"
          ],
          "default": "None",
          "since": "0.1.0"
        },
        {
          "name": "requestTimeoutOpt",
          "type": "scala.concurrent.duration.Duration?",
          "doc": [
            "The timeout for the request. (Default: `None`)"
          ],
          "default": "None",
          "since": "0.1.0"
        },
        {
          "name": "virtualHostOpt",
          "type": "String?",
          "doc": [
            "The virtual host this request will use. (Default: `None`)"
          ],
          "default": "None",
          "since": "0.1.0"
        },
        {
          "name": "proxyServerOpt",
          "type": "ProxyServer?",
          "doc": [
            "The proxy server this request will use. (Default: `None`)"
          ],
          "default": "None",
          "since": "0.1.0"
        }
      ]
    },
